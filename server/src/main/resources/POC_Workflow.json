                        "forkTasks": [
                            [
                                {
                                    "name": "task_10",
                                    "taskReferenceName": "task_10",
                                    "type": "SIMPLE",
                                    "startDelay": 0,
                                    "optional": false
                                },
                                {
                                    "name": "sub_workflow_x",
                                    "taskReferenceName": "wf3",
                                    "inputParameters": {
                                        "mod": "${task_1.output.mod}",
                                        "oddEven": "${task_1.output.oddEven}"
                                    },
                                    "type": "SUB_WORKFLOW",
                                    "startDelay": 0,
                                    "subWorkflowParam": {
                                        "name": "sub_flow_1",
                                        "version": 1
                                    },
                                    "optional": false
                                }
                            ],
                            [
                                {
                                    "name": "task_11",
                                    "taskReferenceName": "task_11",
                                    "type": "SIMPLE",
                                    "startDelay": 0,
                                    "optional": false
                                },
                                {
                                    "name": "sub_workflow_x",
                                    "taskReferenceName": "wf4",
                                    "inputParameters": {
                                        "mod": "${task_1.output.mod}",
                                        "oddEven": "${task_1.output.oddEven}"
                                    },
                                    "type": "SUB_WORKFLOW",
                                    "startDelay": 0,
                                    "subWorkflowParam": {
                                        "name": "sub_flow_1",
                                        "version": 1
                                    },
                                    "optional": false
                                }
                            ]
                        ],
                        "startDelay": 0,
                        "optional": false
                    },
                    {
                        "name": "join",
                        "taskReferenceName": "join2",
                        "type": "JOIN",
                        "startDelay": 0,
                        "joinOn": [
                            "wf3",
                            "wf4"
                        ],
                        "optional": false
                    }
                ]
            },
            "startDelay": 0,
            "optional": false
        },
        {
            "name": "search_elasticsearch",
            "taskReferenceName": "get_es_1",
            "inputParameters": {
                "http_request": {
                    "uri": "http://localhost:9200/conductor/_search?size=10",
                    "method": "GET"
                }
            },
            "type": "HTTP",
            "startDelay": 0,
            "optional": false
        },
        {
