    "tasks": [
        {
            "name": "task_Review",
            "taskReferenceName": "task_1",
            "inputParameters": {
                "mod": "${workflow.input.mod}",
                "oddEven": "${workflow.input.oddEven}",
                "env": {
                    "taskId": "${CPEWF_TASK_ID}",
                    "workflowId": "${HOSTNAME}"
                }
            },
            "type": "SIMPLE",
            "startDelay": 0,
            "optional": false
        },
        {
            "name": "event_task",
            "taskReferenceName": "event_0",
